	        &lt;sp:Code rdf:about="http://www2a.cdc.gov/nip/IIS/IISStandards/vaccines.asp?rpt=vg#TYPHOID"&gt;
	          &lt;rdf:type rdf:resource="http://smartplatforms.org/terms/codes/ImmunizationClass" /&gt; 
