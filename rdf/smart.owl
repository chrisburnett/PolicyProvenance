	        &lt;sp:Code rdf:about="http://smartplatforms.org/terms/codes/ImmunizationRefusalReason#allergy"&gt;
	          &lt;rdf:type rdf:resource="http://smartplatforms.org/terms/codes/ImmunizationRefusalReason" /&gt; 
